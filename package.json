{
  "name": "persson-club",
  "version": "1.0.0",
  "description": "Website for persson.club",
  "main": "index.js",
  "scripts": {
    "build": "grunt build",
    "start": "node -r dotenv/config --optimize_for_size --max_old_space_size=460 --gc_interval=100 index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "grunt dev",
    "server:dev": "nodemon -r dotenv/config --optimize_for_size --max_old_space_size=460 --gc_interval=100 index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "i18n": "^0.13.2",
    "nodejs": "0.0.0",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "grunt": "^1.3.0",
    "grunt-babel": "^8.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^3.1.0",
    "load-grunt-tasks": "^5.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisanen/persson-club.git"
